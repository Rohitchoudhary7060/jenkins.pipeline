pipeline{
    agent any
    stages{
        stage('print'){
            steps{
                echo 'hello dosto'
            }
        }
         stage('update'){
            steps{
               sh 'sudo apt update'
            }
        }
         stage('install git'){
            steps{
               sh 'sudo apt install git -y'
            }
        }
        stage('repo clone'){
            steps{
              sh 'git clone https://github.com/Rohitchoudhary7060/Repolist.git'
            }
        }
        stage(' cd + repo pull'){
            steps{
              sh ''' cd /var/lib/jenkins/workspace/job_1/Repolist
                      git branch -a'''
            }
        }
    }
}
